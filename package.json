{
  "name": "keep-clone-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "debug": "nodemon --watch src -e ts,ejs --exec node -r ts-node/register -r tsconfig-paths/register --inspect ./src/index.ts",
    "dev": "nodemon --watch src -e ts,ejs --exec node -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "start": "node -r ./prod-paths.js dist/index.js",
    "build-ts": "tsc -p tsconfig-build.json",
    "codegen:swagger": "openapi-typescript swagger.yaml --output src/types/swagger/index.ts",
    "seed": "node -r ts-node/register -r tsconfig-paths/register ./tests/seed.ts",
    "test": "NODE_ENV=test node --max_old_space_size=12240 ./node_modules/jest/bin/jest.js --config ./jest.config.js --maxWorkers=2 --passWithNoTests --silent --verbose --colors --json"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "faker": "^5.5.3",
    "firebase-admin": "^10.0.2",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^6.1.4",
    "mongoose-unique-validator": "^3.0.0",
    "npm": "^8.5.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "supertest": "^6.2.2",
    "tsconfig-paths": "^3.14.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/mongoose": "^5.11.97",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.1",
    "jest-environment-node": "^27.4.6",
    "mongodb-memory-server": "6.2.4",
    "nodemon": "^2.0.15",
    "openapi-typescript": "^5.2.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
